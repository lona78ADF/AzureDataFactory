{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF1978"
		},
		"LInkedService_SqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LInkedService_SqlServer'"
		},
		"LinkedService1978_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedService1978'"
		},
		"inputblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'inputblob'"
		},
		"outputsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'outputsql'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF1978_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkedService1978_connectionString": {
						"value": "[parameters('LinkedService1978_connectionString')]"
					},
					"inputblob_connectionString": {
						"value": "[parameters('inputblob_connectionString')]"
					},
					"outputsql_connectionString": {
						"value": "[parameters('outputsql_connectionString')]"
					},
					"LInkedService_SqlServer_connectionString": {
						"value": "[parameters('LInkedService_SqlServer_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}